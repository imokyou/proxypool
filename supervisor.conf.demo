[program:proxy]
command=/home/ptserver/.virtualenvs/proxypool/bin/python /home/ptserver/www/proxypool/run.py
user=ptserver
autostart=true
redirect_stderr=true
stdout_logfile=/var/log/supervisor/proxy.log


[program:proxy]
command=/home/ptserver/.virtualenvs/proxypool/bin/python /home/ptserver/www/proxypool/proxypool/spider.py
user=ptserver
autostart=true
redirect_stderr=true
stdout_logfile=/var/log/supervisor/proxy_spider.log


[program:proxy_api]
command = /home/ptserver/.virtualenvs/proxypool/bin/gunicorn api:app -b localhost:5000 -w 2
directory = /home/ptserver/www/proxypool/proxypool
user = ptserver
stdout_logfile = /var/log/supervisor/proxy_api.log